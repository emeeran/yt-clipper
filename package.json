{
  "name": "obsidian-youtube-clipper",
  "version": "1.4.0",
  "description": "YoutubeClipper - Clip YouTube videos and generate structured notes in Obsidian",
  "main": "main.js",
  "scripts": {
    "dev": "node esbuild.config.mjs",
    "build": "node esbuild.config.mjs production",
    "version": "node version-bump.mjs && git add manifest.json versions.json",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix",
    "format": "prettier --write src/**/*.ts",
    "format:check": "prettier --check src/**/*.ts",
    "type-check": "tsc --noEmit",
    "prepare": "husky"
  },
  "keywords": [
    "obsidian",
    "plugin",
    "youtube",
    "clipper",
    "video",
    "transcript",
    "ai"
  ],
  "author": "Meeran E Mandhini",
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "^29.5.14",
    "@types/node": "^20.11.24",
    "@typescript-eslint/eslint-plugin": "^7.1.1",
    "@typescript-eslint/parser": "^7.1.1",
    "builtin-modules": "3.3.0",
    "esbuild": "0.20.2",
    "eslint": "^8.57.0",
    "husky": "^9.1.7",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^30.2.0",
    "lint-staged": "^15.5.2",
    "obsidian": "latest",
    "prettier": "^3.2.5",
    "ts-jest": "^29.1.2",
    "tslib": "2.6.2",
    "typescript": "5.3.3"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{js,jsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,md,yml,yaml}": [
      "prettier --write"
    ]
  }
}
